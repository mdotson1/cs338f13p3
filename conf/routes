# /admin_portal/departments/{dept}/faculty/{id}
GET         /admin_portal/departments/:dept/faculty/:id                                     controllers.root.admin_portal.departments.department.faculty.professor.Professor.get(dept : String, id : Integer)

# /admin_portal/departments/{dept}/faculty/{id}/semesters
GET         /admin_portal/departments/:dept/faculty/:id/semesters                           controllers.root.admin_portal.departments.department.faculty.professor.semesters.Semesters.get(dept : String, id : Integer)

# /admin_portal/departments/{dept}/faculty/{id}/semesters/{sy}
GET         /admin_portal/departments/:dept/faculty/:id/semesters/:sy                       controllers.root.admin_portal.departments.department.faculty.professor.semesters.semester.Semester.get(dept : String, id : Integer, sy : String)

